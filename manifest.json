{
  "manifest_version": 3,
  "name": "Poids Équipes FFT",
  "version": "1.6",
  "description": "Calcule le poids d'une l'équipe et liste les meilleurs classements depuis les pages tenup.fft.fr.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.tenup.fft.fr/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
